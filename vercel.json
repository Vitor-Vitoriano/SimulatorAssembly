{
    "buildCommand": "npm install && npm run build",
    "outputDirectory": "dist",
    "functions": {
      "api/app.py": {
        "runtime": "python3.12"
      }
    },
    "routes": [
      {
        "src": "/(load|run|step|reset|dump)",
        "dest": "/api/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  